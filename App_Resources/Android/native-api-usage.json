{
    "whitelist-plugins-usages": true,
    "whitelist": [
        "net.lingala.zip4j*:*", "java.io:IOException", "java.lang:Throwable", "java.util:UUID",
        "com.akylas.conty*:*",
        "android.content:ServiceConnection", 
        "android.content.pm:ServiceInfo",
        
        "android.media:MediaMetadataRetriever",

        "android.view:View.OnApplyWindowInsetsListener",
        "android.graphics:Insets",
        "android.app:Notification",
        "android.app:NotificationChannel",
        "android.app:NotificationManager",
        "android.app:PendingIntent",
        "android.app:Service",
        "android.os:Binder",
        "android.os:Environment",
        "androidx.appcompat.app:AppCompatDelegate",
        "androidx.core.app:NotificationCompat",
        "androidx.core.app:NotificationCompat.Builder",
        "androidx.core.app:NotificationCompat.Action",
        "androidx.core.app:NotificationCompat.Action.Builder",
        "androidx.core.app:NotificationCompat.Style",
        "androidx.core.os:LocaleListCompat",
        "androidx.documentfile.provider:DocumentFile",
        "androidx.core.splashscreen:SplashScreen",
        "androidx.core.widget:NestedScrollView",
        "androidx.media.app:NotificationCompat.MediaStyle",
        "androidx.media.app:NotificationCompat",
        "android.support.v4.media:MediaMetadataCompat",
        "android.support.v4.media:MediaMetadataCompat.Builder",
        "android.support.v4.media.session:MediaControllerCompat",
        "android.support.v4.media.session:MediaSessionCompat",
        "android.support.v4.media.session:MediaSessionCompat.Token",
        "android.support.v4.media.session:MediaSessionCompat.Callback",
        "android.support.v4.media:MediaDescriptionCompat",
        "android.support.v4.media.session:PlaybackStateCompat",
        "android.support.v4.media.session:PlaybackStateCompat.Builder",
        "android.support.v4.media.session:PlaybackStateCompat.CustomAction",
        "android.support.v4.media.session:PlaybackStateCompat.CustomAction.Builder",
        "androidx.media.session:MediaButtonReceiver",
        "android.text.format:DateFormat",

        "java.lang.reflect:Array",
        "java.lang:Iterable",

        "android.app:Dialog",
        "android.content:DialogInterface.OnKeyListener"
    ],
    "blacklist": [
        "android.widget:DatePicker",
        "android.widget:DatePicker.OnDateChangedListener",
        "android.widget:NumberPicker",
        "android.widget:NumberPicker.Formatter",
        "android.widget:NumberPicker.OnValueChangeListener",
        "android.widget:TabHost",
        "android.widget:TabHost.OnTabChangeListener",
        "android.widget:TabHost.TabContentFactory",
        "android.widget:TabHost.TabSpec",
        "android.widget:TabWidget",
        "android.widget:TimePicker",
        "android.widget:TimePicker.OnTimeChangedListener",
        "androidx.appcompat.widget:SearchView",
        "androidx.appcompat.widget:SearchView.OnCloseListener",
        "androidx.appcompat.widget:SearchView.OnQueryTextListener",
        "androidx.appcompat.widget:Toolbar.OnMenuItemClickListener",
        "androidx.appcompat.widget:AppCompatTextView",

        "org.ow2.asmdex*:*",
        "org.nativescript.widgets:TabItemSpec*",
        "org.nativescript.widgets:TabsBar*",
        "org.nativescript.widgets:SegmentedBar*",
        "org.nativescript.widgets:TabViewPager*",
        "org.nativescript.widgets:TabLayout*",
        "org.nativescript.widgets:BottomNavigationBar*",
        "com.google.android.material.chip*:*",
        "com.google.android.material.appbar*:*",
        "com.google.android.material.floatingactionbutton*:*",
        "com.google.android.material.bottomappbar*:*",
        "com.google.android.material.internal*:*",
        "com.google.android.material.transformation*:*",
        "com.google.android.material.theme*:*",
        
        "*:*BuildConfig",
        "*:R.*",
        "*:R",
        "*:R$"
    ],
    "proguard_blacklist": [
        "org.nativescript.widgets:TabItemSpec*",
        "org.nativescript.widgets:TabsBar*",
        "org.nativescript.widgets:SegmentedBar*",
        "org.nativescript.widgets:TabViewPager*",
        "org.nativescript.widgets:TabLayout*",
        "org.nativescript.widgets:BottomNavigationBar*",
        "com.google.android.material.chip*:*",
        "com.google.android.material.appbar*:*",
        "com.google.android.material.floatingactionbutton*:*",
        "com.google.android.material.bottomappbar*:*",
        "com.google.android.material.internal*:*",
        "com.google.android.material.transformation*:*",
        "com.google.android.material.theme*:*"
    ]
